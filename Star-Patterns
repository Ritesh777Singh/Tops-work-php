*




#include <stdio.h>
int main() {
    int num = 5;
    for(int row = 1; row <= num; row++){
        for(int col = 1; col <= row; col++){
            printf("* ");
        }
        printf("\n");
    }
   
 
    for(int row = 1; row <= num; row += 1){
        for(int col = num; col > row; col--){
            printf("* ");
        }
        printf("\n");
    }
    

    return 0;
}
